<%= form.fields_for symbol do |fields| %>
  <div>
      <%= fields.label :year %>
      <%= fields.number_field :year, size: 4, value: form.object[symbol]&.year %>
      <%= fields.label :month %>
      <%= fields.number_field :month, size: 2, min: 1, max: 12, value: form.object[symbol]&.month %>
      <%= fields.label :day %>
      <%= fields.number_field :day, size: 2, min: 1, max: 31, value: form.object[symbol]&.day %>
  </div>
<% end %>

